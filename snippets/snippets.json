	"amp-selector script": {
		"prefix": "amp-selector-script",
		"body": [
			"<script async custom-element=\"amp-selector\" src=\"https://cdn.ampproject.org/v0/amp-selector-0.1.js\"></script>$0"
		]
	},

	"amp-selector": {
		"prefix": "amp-selector",
    "body": [
			"<amp-selector",
