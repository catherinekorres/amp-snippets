	"amp-sidebar": {
		"prefix": "amp-sidebar",
    "body": [
			"<amp-sidebar id=\"sidebar-left\" class=\"sample-sidebar\" layout=\"nodisplay\" side=\"left\">",
			"  <h3>Sidebar</h3>",
			"  <button on=\"tap:sidebar-left.close\">Close sidebar</button>",
			"  <nav>",
			"    <ul>",
			"      <li>Nav item 1</li>",
			"      <li>Nav item 2</li>",
			"      <li>Nav item 3</li>",
			"      <li>Nav item 4</li>",
			"    </ul>",
			"  </nav>",
			"</amp-sidebar>",
			"<button on=\"tap:sidebar-left.toggle\">Toggle sidebar</button>$0"
		]
	},

