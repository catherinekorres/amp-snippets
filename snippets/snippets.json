	"amp-accordion script": {
		"prefix": "amp-accordion-script",
		"body": "<script async custom-element=\"amp-accordion\" src=\"https://cdn.ampproject.org/v0/amp-accordion-0.1.js\"></script>$0"
	},

	"amp-accordion": {
		"prefix": "amp-accordion",
    "body": [
			"<amp-accordion>",
			"	<section>",
			"		<h4>${1:SECTION_TITLE}</h4>",
			"		<div>${2:CONTENT}</div>",
			"	</section>",
			"	<section>",
			"		<h4>${3:SECTION_TITLE}</h4>",
			"		<div>${4:CONTENT}</div>",
			"	</section>",
			"</amp-accordion>$0"
		]
	},

	"amp-analytics script": {
		"prefix": "amp-analytics-script",
		"body": "<script async custom-element=\"amp-analytics\" src=\"https://cdn.ampproject.org/v0/amp-analytics-0.1.js\"></script>$0"
	},

	"amp-analytics": {
		"prefix": "amp-analytics",
    "body": [
			"<amp-analytics type=\"gtag\" data-credentials=\"include\">",
			"	<script type=\"application/json\">",
			"		{",
			"			\"vars\": {",
			"				\"gtag_id\": \"${1:GA_MEASUREMENT_ID}\",",
			"				\"config\": {",
			"					\"${1:GA_MEASUREMENT_ID}\": {",
			"						\"groups\": \"default\"",
			"					}",
			"				}",
			"			},",
			"			\"triggers\": {",
			"				\"${2:TRIGGER_NAME}\": {",
			"					\"selector\": \"#${3:SELECTOR_ID}\",",
			"					\"on\": \"${4:EVENT}\",",
			"					\"vars\": {",
			"						\"event_name\": \"$5\",",
			"						\"method\": \"$6\"",
			"					}",
			"				}",
			"			}",
			"		}",
			"	</script>",
			"</amp-analytics>$0"
		]
	},

	"amp-bind script": {
		"prefix": "amp-bind-script",
		"body": "<script async custom-element=\"amp-bind\" src=\"https://cdn.ampproject.org/v0/amp-bind-0.1.js\"></script>$0"
	},

	"amp-carousel script": {
		"prefix": "amp-carousel-script",
		"body": "<script async custom-element=\"amp-carousel\" src=\"https://cdn.ampproject.org/v0/amp-carousel-0.1.js\"></script>$0"
	},

	"amp-carousel": {
		"prefix": "amp-carousel",
    "body": [
			"<amp-carousel width=\"$1\"",
			"  height=\"$2\"",
			"  layout=\"responsive\"",
			"  type=\"slides\">",
			"  <amp-img src=\"$3\"",
			"    width=\"$1\"",
			"    height=\"$2\"",
			"    layout=\"responsive\"",
			"    alt=\"$4\"></amp-img>",
			"  <amp-img src=\"$7\"",
			"    width=\"$1\"",
			"    height=\"$2\"",
			"    layout=\"responsive\"",
			"    alt=\"$5\"></amp-img>",
			"  <amp-img src=\"$11\"",
			"    width=\"$1\"",
			"    height=\"$2\"",
			"    layout=\"responsive\"",
			"    alt=\"$6\"></amp-img>",
			"</amp-carousel>$0"
		]
	},

	"amp-facebook script": {
		"prefix": "amp-facebook-script",
		"body": "<script async custom-element=\"amp-facebook\" src=\"https://cdn.ampproject.org/v0/amp-facebook-0.1.js\"></script>$0"
	},

	"amp-facebook": {
		"prefix": "amp-facebook",
    "body": [
			"<amp-facebook width=\"$1\" height=\"$2\"",
			"  layout=\"responsive\"",
			"  data-embed-as=\"$3\"",
			"  data-href=\"https://www.facebook.com/${4:CONTENT_URL}\">",
			"</amp-facebook>$0"
		]
	},

	"amp-facebook-page script": {
		"prefix": "amp-facebook-page-script",
		"body": "<script async custom-element=\"amp-facebook-page\" src=\"https://cdn.ampproject.org/v0/amp-facebook-page-0.1.js\"></script>$0"
	},

	"amp-facebook-page": {
		"prefix": "amp-facebook-page",
    "body": [
			"<amp-facebook-page width=\"$1\"",
			"  height=\"$2\"",
			"  layout=\"responsive\"",
			"  data-tabs=\"timeline, events\"",
			"  data-href=\"https://www.facebook.com/${3:PAGE_URL}\">",
			"</amp-facebook-page>$0"
		]
	},

	"amp-font script": {
		"prefix": "amp-font-script",
		"body": "<script async custom-element=\"amp-font\" src=\"https://cdn.ampproject.org/v0/amp-font-0.1.js\"></script>$0"
	},

	"amp-font": {
		"prefix": "amp-font",
    "body": [
			"<amp-font",
			"  layout=\"nodisplay\"",
			"  font-family=\"$1\"",
			"  timeout=\"$2\"",
			"  on-error-remove-class=\"$3\"",
			"  on-error-add-class=\"$4\"",
			"  on-load-remove-class=\"$5\"",
			"  on-load-add-class=\"$6\">",
			"</amp-font>$0"
		]
	},

	"amp-form script": {
		"prefix": "amp-form-script",
		"body": "<script async custom-element=\"amp-form\" src=\"https://cdn.ampproject.org/v0/amp-form-0.1.js\"></script>$0"
	},

	"amp-form": {
		"prefix": "amp-form",
    "body": [
			"<form method=\"post\"",
			"  action-xhr=\"$1\"",
			"  target=\"_top\">",
			"  <fieldset>",
			"    <label>",
			"      <span>Name:</span>",
			"      <input type=\"text\"",
			"        name=\"name\"",
			"        required>",
			"    </label>",
			"    <label>",
			"      <span>Email:</span>",
			"      <input type=\"email\"",
			"        name=\"email\"",
			"        required>",
			"    </label>",
			"    <input type=\"submit\"",
			"      value=\"Subscribe\">",
			"  </fieldset>",
			"  <div submit-success>",
			"    <template type=\"amp-mustache\">",
			"     Subscription successful!",
			"    </template>",
			"  </div>",
			"  <div submit-error>",
			"    <template type=\"amp-mustache\">",
			"      Subscription failed!",
			"    </template>",
			"  </div>",
			"</form>$0",
		]
	},

	"amp-iframe script": {
		"prefix": "amp-iframe-script",
		"body": "<script async custom-element=\"amp-iframe\" src=\"https://cdn.ampproject.org/v0/amp-iframe-0.1.js\"></script>$0"
	},

	"amp-iframe": {
		"prefix": "amp-iframe",
    "body": [
			"<amp-iframe width=\"$1\" height=\"$2\"",
			"	sandbox=\"allow-scripts allow-same-origin\"",
			"	layout=\"responsive\"",
			"	frameborder=\"0\"",
			"	src=\"$3\">",
			"</amp-iframe>$0"
		]
	},

	"amp-image-lightbox script": {
		"prefix": "amp-image-lightbox-script",
		"body": "<script async custom-element=\"amp-image-lightbox\" src=\"https://cdn.ampproject.org/v0/amp-image-lightbox-0.1.js\"></script>$0"
	},

	"amp-image-lightbox": {
		"prefix": "amp-image-lightbox",
    "body": [
			"<amp-img on=\"tap:lightbox1\"",
			"  role=\"button\"",
			"  tabindex=\"0\"",
			"  src=\"$1\"",
			"  alt=\"$2\"",
			"  title=\"$3\"",
			"  layout=\"responsive\"",
			"  width=\"$4\"",
			"  height=\"$5\"></amp-img>",
			"<amp-image-lightbox id=\"lightbox1\"",
			"  layout=\"nodisplay\"></amp-image-lightbox>$0"
		]
	},

	"amp-img": {
		"prefix": "amp-img",
    "body": [
			"<amp-img src=\"$1\"",
			"  width=\"$2\"",
			"  height=\"$3\"",
			"  layout=\"responsive\"",
			"  alt=\"$4\">",
			"</amp-img>$0"
		]
	},

	"amp-instagram script": {
		"prefix": "amp-instagram-script",
		"body": "<script async custom-element=\"amp-instagram\" src=\"https://cdn.ampproject.org/v0/amp-instagram-0.1.js\"></script>$0"
	},

	"amp-instagram": {
		"prefix": "amp-instagram",
    "body": [
			"<amp-instagram",
			"  data-shortcode=\"${1:fBwFP}\"",
			"  width=\"1\"",
			"  height=\"1\"",
			"  layout=\"responsive\">",
			"</amp-instagram>$0"
		]
	},

	"amp-install-serviceworker script": {
		"prefix": "amp-install-serviceworker-script",
		"body": "<script async custom-element=\"amp-install-serviceworker\" src=\"https://cdn.ampproject.org/v0/amp-install-serviceworker-0.1.js\"></script>$0"
	},

